												"physicalType": "String"
											},
											"sink": {
												"name": "orderid",
												"type": "String",
												"physicalType": "UTF8"
											}
										},
										{
											"source": {
												"name": "orderdate",
												"type": "String",
												"physicalType": "String"
											},
											"sink": {
												"name": "orderdate",
												"type": "String",
												"physicalType": "UTF8"
											}
										},
										{
											"source": {
												"name": "customer",
												"type": "String",
