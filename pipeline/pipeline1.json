											"sink": {
												"name": "product",
												"type": "String",
												"physicalType": "UTF8"
											}
										},
										{
											"source": {
												"name": "units",
												"type": "String",
												"physicalType": "String"
											},
											"sink": {
												"name": "units",
												"type": "String",
												"physicalType": "UTF8"
											}
										},
										{
											"source": {
												"name": "billingaddress",
												"type": "String",
												"physicalType": "String"
											},
											"sink": {
												"name": "billingaddress",
												"type": "String",
												"physicalType": "UTF8"
											}
										},
										{
											"source": {
												"name": "remarks",
