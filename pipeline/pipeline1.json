											"sink": {
												"name": "remarks",
												"type": "String",
												"physicalType": "UTF8"
											}
										}
									],
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "orderinput",
									"type": "DatasetReference",
									"parameters": {
										"inputContainer": {
											"value": "@activity('Azure Function1').output.containerName",
											"type": "Expression"
										},
										"folderPath": {
											"value": "@activity('Azure Function1').output.path",
											"type": "Expression"
										},
										"fileName": {
											"value": "@activity('Azure Function1').output.blobName",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "orderoutputparquet",
									"type": "DatasetReference",
									"parameters": {
